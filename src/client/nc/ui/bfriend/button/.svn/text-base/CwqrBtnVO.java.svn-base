package nc.ui.bfriend.button;

import nc.ui.trade.base.IBillOperate;
import nc.vo.trade.button.ButtonVO;

public class CwqrBtnVO {
	public static final String cwqrBtnVO= "财务确认";
	
	public  ButtonVO getButtonVO(){
        ButtonVO btnvo = new ButtonVO();
        btnvo.setBtnCode("CwqrBtnVO");
        btnvo.setBtnNo(IdhButton.CWQR);
        btnvo.setBtnName(cwqrBtnVO);
        btnvo.setHintStr(cwqrBtnVO);
        btnvo.setBtnChinaName(cwqrBtnVO);
        
        btnvo.setOperateStatus( new int [] {IBillOperate.OP_NOTEDIT });
        
         return btnvo;            
  }
}
