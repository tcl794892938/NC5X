import nc.vo.trade.button.ButtonVO;

public class SealBtnVO {
	public static final String sealBtnVO= "╦губ";
	
	public ButtonVO getButtonVO(){
		ButtonVO btnvo = new ButtonVO();
		btnvo.setBtnCode("SealBtnVO");
		btnvo.setBtnNo(IdhButton.SEAL);
		btnvo.setBtnName(sealBtnVO);
