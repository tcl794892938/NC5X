package nc.ui.dahuan.exceltools;

/**
 * 无效的变量异常信息
 * 
 * @author sun_jian
 * @version 1.0.0
 */
public class InvalidVariableException extends Exception {

	private static final long serialVersionUID = 1L;

	/**
	 * 构造函数
	 * 
	 * @param java.lang.String
	 *            message
	 */
	public InvalidVariableException(String message) {

		super(message);

	}

	/**
	 * 构造函数
	 * 
	 * @param java.lang.String
	 *            message
	 * @param java.lang.Throwable
	 *            e
	 */
	public InvalidVariableException(String message, Throwable e) {

		super(message, e);

	}

	/**
	 * 构造函数
	 * 
	 * @param java.lang.Throwable
	 *            e
	 */
	public InvalidVariableException(Throwable e) {

		super(e);

	}

}
